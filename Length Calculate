#include <iostream>
using namespace std;

int leg_cal(char a[]){
    int length=0;
    
    while(true){
        if(length[a]=='\0'){
            break;
        }
        length++;
    }
    return length;
}


int main(){

   char name[50]={};
   cout<<"Enter Name: ";
   cin>>name;
   cout<<"Length is: "<<leg_cal(name)<<endl;


    return 0;
}
