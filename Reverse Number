//Reverse Number
#include <iostream>
using namespace std;

void digitsum(int num);
int main()
{
    int num = 0, rev = 0;
    cout << "Enter a number: ";
    cin >> num;
    digitsum(num);

    return 0;
}

void digitsum(int num)
{

    int digit = 0, i = 0;
    while (num != 0)
    {
        digit = num % 10;
        cout << digit;
        num = num / 10;
    }

}


