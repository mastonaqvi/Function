#include <iostream>
using namespace std;

void swap(int& num1, int& num2);
int main()
{
    int num1 = 0, num2 = 0;

    cout << "Enter a number: ";
    cin >> num1;
    cout << "Enter a number: ";
    cin >> num2;

    swap(num1, num2);

    cout << "the swap vales are  " << endl << num1 << " and " << num2;

    return 0;
}

void swap(int& num1, int& num2)
{
    int temp = 0;
    temp = num1;
    num1 = num2;
    num2 = temp;

}


